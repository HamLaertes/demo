<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.securitydemo.dao.SysRoleDao">

    <select id="getRoleById" parameterType="long" resultType="com.securitydemo.domain.SysRole">
        SELECT id, name
        FROM sys_role
        WHERE id=#{id}
    </select>
</mapper>